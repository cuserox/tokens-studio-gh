{
  "Fam": {
    "$type": "fontFamilies",
    "$value": "Inter"
  },
  "Base": {
    "$type": "typography",
    "$value": {
      "fontFamily": "{Fam}",
      "fontSize": "12px",
      "fontWeight": "Bold"
    }
  },
  "Bold": {
    "$extensions": {
      "studio.tokens": {
        "modify": {}
      }
    },
    "$type": "fontWeights",
    "$value": "Bold"
  },
  "hot-pink": {
    "$type": "color",
    "$value": "#fc04b2"
  },
  "hot-purple": {
    "$type": "color",
    "$value": "#9602fc"
  },
  "pink-borderr": {
    "$extensions": {
      "studio.tokens": {
        "modify": {}
      }
    },
    "$type": "border",
    "$value": {
      "color": "{hot-pink}",
      "width": "1px",
      "style": "solid"
    }
  },
  "Small": {
    "$type": "typography",
    "$value": {
      "fontFamily": "{Fam}",
      "fontSize": "8px"
    }
  },
  "hello-world-theme": {
    "base-text": {
      "idle-status": {
        "square": {
          "$type": "composition",
          "$value": {
            "typography": "{Base}",
            "minWidth": "100px"
          }
        }
      }
    },
    "idle-status": {
      "square-cta": {
        "title": {
          "$type": "typography",
          "$value": "{very-long-square.wrapping-text-behaviour-token-name.white-background-label-action.ghost-variant.large-cta-just-for-testing-purposes.try-and-break-this-search-box}"
        }
      }
    },
    "a-very-long-color-name": {
      "try-to-break": {
        "square-cta": {
          "$type": "color",
          "$value": "#14f805"
        }
      }
    }
  },
  "very-long-square": {
    "wrapping-text-behaviour-token-name": {
      "white-background-label-action": {
        "ghost-variant": {
          "large-cta-just-for-testing-purposes": {
            "try-and-break-this-search-box": {
              "$extensions": {
                "studio.tokens": {
                  "modify": {}
                }
              },
              "$type": "typography",
              "$value": {
                "fontFamily": "Arial",
                "fontWeight": "500",
                "fontSize": "14px",
                "lineHeight": "1.5"
              }
            }
          }
        }
      }
    }
  },
  "give-me-a-super-long-color-name-to-break-the-search-like-highlight-button": {
    "$type": "color",
    "$value": "#f08080"
  },
  "asdfghjkl;sdfghjkldfghjkldfghjklzfgdfghfdghsdghdfghdfghfgsdfgasdfsdfgsdghsdghdfghsdghsdf": {
    "$type": "typography",
    "$value": {
      "fontFamily": "{Fam}"
    }
  },
  "a-new-color-to-push": {
    "$type": "color",
    "$value": "#eef492"
  }
}