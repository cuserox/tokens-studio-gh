{
  "Fam": {
    "$type": "fontFamilies",
    "$value": "Inter"
  },
  "Base": {
    "$type": "typography",
    "$value": {
      "fontFamily": "{Fam}",
      "fontSize": "12px",
      "fontWeight": "Bold"
    }
  },
  "Bold": {
    "$extensions": {
      "studio.tokens": {
        "modify": {}
      }
    },
    "$type": "fontWeights",
    "$value": "Bold"
  },
  "hot-pink": {
    "$type": "color",
    "$value": "#fc04b2"
  },
  "hot-purple": {
    "$type": "color",
    "$value": "#9602fc"
  },
  "pink-border": {
    "$type": "border",
    "$value": {
      "color": "{hot-pink}",
      "width": "1px",
      "style": "solid"
    }
  },
  "Small": {
    "$type": "typography",
    "$value": {
      "fontFamily": "{Fam}",
      "fontSize": "8px"
    }
  },
  "hello-world-theme": {
    "base-text": {
      "idle-status": {
        "square": {
          "$type": "composition",
          "$value": {
            "typography": "{Base}",
            "minWidth": "100px"
          }
        }
      }
    }
  }
}